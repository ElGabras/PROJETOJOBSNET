                        },
                        "endereco": {
                            "type": "string"
                        },
                        "celular": {
                            "type": "string"
                        },
                        "Tel1": {
                            "type":"string"
                        },
                        "cep": {
                            "type": "string"
                        }
                     }   
                 },
